extends МашинаРеквизитов.КомандыУправления.Интерфейс;

  Import МашинаРеквизитов;
  Import Базовый;

inclass

  proc СоздатьКоманды;
   var Иконка: string;
   Иконка = BaseInfo.ProjectByName["Первичка"].Folder+"Приложения\Иконки\";

   extended СоздатьКоманды;
      with КомандыУправления.ОткрытьБланк do
      Добавить("ЗакрытыйПериод.блЗакрытиеПериода",         "Дата Закрытие Периода",      Настройки.блЗакрытиеПериода,, Иконка + "Инструменты");
      end;

  end;

  proc СоздатьПанели;
    extended СоздатьПанели;

    with КомандыУправления.ПанельКоманд.Взять("Сервис") do
      with ВзятьГруппу("Сервис",,,60) do
        ДобавитьКоманды([
                          "-Закрытие Периода",
                          "ЗакрытыйПериод.блЗакрытиеПериода"
                         ]);
      end;
    end;
   end;



inobject

end